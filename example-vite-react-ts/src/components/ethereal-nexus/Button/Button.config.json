{
  "version": "1.0.0",
  "description": "Generic button component",
  "dialog": [
    {
      "type": "textfield",
      "name": "name",
      "id": "button-name",
      "label": "Name",
      "placeholder": "The name of the button"
    },
    {
      "type": "textfield",
      "name": "label",
      "id": "button-label",
      "label": "Label",
      "placeholder": "The label of the button"
    },
    {
      "type": "textfield",
      "name": "url",
      "id": "button-url",
      "label": "Button URL",
      "placeholder": "An external URL that will be opened on button click"
    },
    {
      "type": "textfield",
      "name": "type",
      "id": "button-type",
      "label": "Button Type",
      "placeholder": "The html button type (submit | reset | button)"
    },
    {
      "type": "textfield",
      "name": "style",
      "id": "button-style",
      "label": "Button style",
      "placeholder": "The html css style (default | primary)"
    }
  ]
}